{
    "name": "create-next-tombv3",
    "version": "0.8.16",
    "author": "TombLabs",
    "description": "Next.JS starter with wallet adapter included using typescript",
    "repository": "https://github.com/Technox25/create-next-tomb",
    "bin": "./bin.js",
    "files": [
        "public",
        "src",
        ".editorconfig",
        ".eslintrc.json",
        ".gitignore",
        ".prettierignore",
        ".prettierrc",
        "LICENSE",
        "next.config.js",
        "next-env.d.ts",
        "package.json",
        "README.md",
        "tsconfig.json"
    ],
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "build": "tsc --build --verbose && next build",
        "clean": "shx rm -rf .next .swc lib",
        "dev": "NODE_OPTIONS='--inspect' next dev",
        "start": "next start",
        "lint": "prettier --check 'src/{*,**/*}.{ts,tsx,js,jsx,json}' && eslint && next lint"
    },
    "dependencies": {
        "@metaplex-foundation/js": "^0.18.1",
        "@solana/wallet-adapter-base": "^0.9.21",
        "@solana/wallet-adapter-react": "^0.15.26",
        "@solana/wallet-adapter-react-ui": "^0.9.24",
        "@solana/wallet-adapter-wallets": "^0.19.9",
        "@solana/web3.js": "^1.73.2",
        "axios": "^1.3.3",
        "daisyui": "^2.50.2",
        "next": "^12.2.5",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "swiper": "^9.0.5"
    },
    "devDependencies": {
        "@types/node-fetch": "^2.6.2",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "autoprefixer": "^10.4.8",
        "eslint": "^8.22.0",
        "eslint-config-next": "^12.2.5",
        "postcss": "^8.4.16",
        "prettier": "^2.7.1",
        "shx": "^0.3.4",
        "tailwindcss": "^3.1.8",
        "typescript": "~4.7.4"
    }
}
